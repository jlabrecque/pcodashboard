<%= semantic_form_for [:admin, @conncard] do |f| %>
  <%= f.inputs do %>
     <%= f.input :name %>
     <%= f.input :campus, :as => :select %>
     <%= f.fields_for :workflows do |builder| %>
       <%= render "question", :f => builder %>
     <% end %>
   <% end %>
  <%= f.submit "Create" %>
<% end %>
